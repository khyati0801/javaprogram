		TreeMap<Integer,String> Tm=new TreeMap<Integer,String>(map);
		
		Iterator i=Tm.keySet().iterator();
		
//		while(i.hasNext()) {
//			int key=(int)i.next();
//			
//			
//			System.out.println(key+""+map.get(key));
//			
//		}
		
		while(i.hasNext()) {
			String value=(String)i.next();
			
			
			System.out.println(value+""+map.get(value));
			
		}
		
		

	}

}
