package com.file;

import java.io.File;
import java.io.IOException;

public class FileEx {

	public static void main(String[] args) throws IOException {
		  File file = new File("javaFile123.txt");  
          if (file.exists()) {  
		      System.out.println(true);  
		  } else {  
		      System.out.println(false);  
		  }  

	}

}
