package java8Lamda;

public class LamdaMulti {

	public static void main(String[] args) {
		Runnable r=()-> { 
			
			for(int i=0;i<5;i++) {
				
				System.out.println("child Thread");
				
				
			}};
			Thread t=new Thread();
			t.start();
		for(int i=0;i<5;i++) 
			
			System.out.println("main thread");
			
		
	}	

}